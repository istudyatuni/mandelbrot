    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.7",
    "eslint": "^8.15.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "redrun": "^10.0.1",
    "sass": "^1.51.0",
    "svelte": "^3.48.0",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.3",
    "svelte-storages": "^0.0.2",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.9",
