    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.16",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "husky": "^7.0.4",
    "jest": "^26.4.7",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "redrun": "^9.0.1",
    "sass": "^1.49.8",
    "svelte": "^3.46.3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.3",
    "svelte-storages": "^0.0.2",
    "tailwindcss": "^3.0.18",
    "vite": "^2.8.4",
    "vite-plugin-wasm": "^0.0.2"
  }
